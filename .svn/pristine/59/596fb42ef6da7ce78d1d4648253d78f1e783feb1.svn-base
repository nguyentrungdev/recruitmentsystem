/**
 * Box-sizing: border-box;
 * Apply a natural box layout model to all elements, but allowing components to change 
 */
html {
	box-sizing: border-box;
}

*, *:before, *:after {
	box-sizing: inherit;
}

/* CLEARFIX HACK */
/**
 * For modern browsers
 * 1. The space content is one way to avoid an Opera bug when the
 *    contenteditable attribute is included anywhere else in the document.
 *    Otherwise it causes space to appear at the top and bottom of elements
 *    that are clearfixed.
 * 2. The use of `table` rather than `block` is only necessary if using
 *    `:before` to contain the top-margins of child elements.
 */
.clearfix:before, .clearfix:after {
	content: " "; /* 1 */
	display: table; /* 2 */
}

.clearfix:after {
	clear: both;
}

/**
 * For IE 6/7 only
 * Include this rule to trigger hasLayout and contain floats.
 */
.clearfix {
	*zoom: 1;
}

/**
 *  LET THE IMAGES BEHAVE */
img {
	width: 100%;
	max-width: 100%;
}

/*  Basic CSS Setup */
/* Remove padding and margin */
* {
	margin: 0;
	padding: 0;
	border: 0;
}

.spacing {
	margin: 20px auto;
}

.dashed {
	border: 2px dashed #dddddd;
}

.clear {
	clear: both;
}

.right {
	float: right;
}

.left {
	float: left;
}

body {
	font-family: 'open_sansregular', sans-serif;
	font-size: 1.2em;
	color: #000;
}

h1 {
	font-size: 2.2em;
	font-family: 'open_sansbold', sans-serif;
}

h2 {
	font-size: 2.0em;
	font-family: 'open_sansbold', sans-serif;
}

h3 {
	font-size: 1.8em;
	font-family: 'open_sansbold', sans-serif;
}

h4 {
	font-size: 1.6em;
	font-family: 'open_sanssemibold', sans-serif;
}

h5 {
	font-size: 1.4em;
	font-family: 'open_sanssemibold', sans-serif;
}

p {
	font-size: 1em;
}

/* Tables  
-----------------------------------------------------------------------------*/
table {
	border-spacing: 0;
	border-collapse: collapse;
}

td {
	text-align: left;
	font-weight: normal;
	vertical-align: baseline;
	white-space: pre-line;
	padding: 2px;
	word-break: break-all;
}

/* table>tbody>tr>td, table>tbody>tr>th, table>tfoot>tr>td, table>tfoot>tr>th, */
/* 	table>thead>tr>td, table>thead>tr>th { */
/* 	padding: 8px; */
/* line-height: 1.42857; */
/* vertical-align: top; */
/* border-top: 1px solid #DDD; */
/* }  */
/* YOUR STYLES
------------------------------------------------------------------------------*/
/* Behavior effect */
/* TYPOGRAPHY */
.ui-widget {
	font-family: "open_sansregular", sans-serif;
}

.glyphicon {
	line-height: 1.2em;
}

label.ui-widget, .label, .control-label {
	font-family: "open_sanssemibold", sans-serif;
	color: #000;
	display: inline;
	padding: 0.2em 0.6em 0.3em;
	font-size: 100%;
	font-weight: 400;
	line-height: 1;
	text-align: left;
	white-space: nowrap;
	vertical-align: middle;
	border-radius: 0.25em;
}

.ui-selectonemenu-label.ui-inputfield {
	padding: 5px 10px;
}

.no-padding {
	padding-left: 0px;
	padding-right: 0px;
}

.no-padding-left {
	padding-left: 0px;
}

.no-padding-right {
	padding-right: 0px;
}
/* Form Elements CSS */
.ui-outputlabel .ui-outputlabel-rfi, .required {
	color: red;
	margin-left: 0.2em;
}

input, .ui-inputfield {
	border: 2px #dddddd solid;
	padding: 5px 10px 5px;
	width: 100%;
}

input.ui-state-error, .ui-inputfield.ui-state-error {
	border: 2px solid #FF5245;
}

.ui-selectmanycheckbox label, .ui-selectoneradio label {
	font-weight: 400;
}

.chkbox-label {
	font-weight: 400;
	line-height: 40px;
}

.radio-label {
	font-weight: 400;
	line-height: 35px;
}

/* SELECT ONE MENU, SELECT LIST BOX*/
.ui-selectonemenu {
	width: 100% !important;
	border: 2px #dddddd solid;
	padding: 5px 0px;
}

.ui-selectonemenu .ui-selectonemenu-trigger {
	background-color: rgb(241, 241, 241);
	border-left: 2px solid rgb(221, 221, 221);
}

.ui-selectonemenu .ui-selectonemenu-trigger .ui-icon {
	margin-left: -5px;
	margin-top: 10px;
}

.ui-selectonemenu-panel .ui-selectonemenu-list-item:hover {
	background-color: #f1f1f1;
}

.ui-selectonemenu-panel.ui-widget-content {
	border: 1px solid #dddddd;
}

.ui-selectonelistbox.ui-widget-content {
	padding: 3px;
}

.ui-selectonelistbox .ui-selectlistbox-item, .ui-selectmanymenu .ui-selectlistbox-item,
	.ui-multiselectlistbox .ui-multiselectlistbox-item {
	padding: 2px 10px;
}

.ui-selectonelistbox .ui-selectlistbox-item:hover, .ui-selectmanymenu .ui-selectlistbox-item:hover,
	.ui-multiselectlistbox .ui-multiselectlistbox-item:hover {
	background-color: #f1f1f1;
}

/* INPUT, CHECKBOX, RADIO BUTTON */
input:focus, .ui-inputfield:focus, .ui-radiobutton .ui-radiobutton-box.ui-state-active,
	.ui-chkbox .ui-chkbox-box.ui-state-active {
	border: 2px solid #33B6BC;
}

button, .ui-state-default.ui-button {
	border: none;
	color: #fff;
	background-color: #0096D6;
	border-radius: 3px;
	font-family: "open_sanssemibold", sans-serif;
	min-height: 32px;
}

button:hover, .ui-state-default.ui-button:hover {
	background-color: #09B1F9;
}

a, a.ui-widget-content, .ui-link, .ui-widget-content a {
	color: #0096D6;
}

.ui-radiobutton-icon, .ui-chkbox-icon {
	margin-left: -1.5px;
	margin-top: -3px;
}

.ui-radiobutton .ui-radiobutton-box, .ui-chkbox .ui-chkbox-box {
	border: 2px solid #dddddd;
}

/* Grid CSS */
.ui-grid-col-1, .ui-grid-col-2, .ui-grid-col-3, .ui-grid-col-4,
	.ui-grid-col-5, .ui-grid-col-6, .ui-grid-col-7, .ui-grid-col-8,
	.ui-grid-col-9, .ui-grid-col-10, .ui-grid-col-11, .ui-grid-col-12 {
	padding: 0px 15px;
}

/* Fieldset CSS*/
.ui-fieldset-content tr {
	height: 50px;
}

.ui-fieldset, .ui-fieldset .ui-fieldset-legend {
	border: none;
}

legend {
	display: block;
	width: auto;
	padding: 0px;
	margin-bottom: 0px;
	font-size: 21px;
	line-height: 2.188em;
	color: #333;
	border: unset;
	font-size: 21px;
	font-family: "open_sanssemibold", sans-serif;
}

.ui-widget-content {
	border: 2px #DEDEDE solid;
}

/* TAB */
.ui-tabs .ui-tabs-nav {
	border-radius: 0px;
	background: #fff none repeat scroll 0% 0%;
	border: none;
}

.ui-tabs.ui-widget-content {
	border: medium none;
}

.ui-tabs-panels {
	border: 1px solid #dddddd;
}

.ui-tabs.ui-tabs-top>.ui-tabs-nav li.ui-tabs-selected {
	background-color: #F1F1F1;
}

.ui-tabs.ui-tabs-top>.ui-tabs-nav li {
	background-color: #D4D5D5;
}

/* TOOLTIPS, DIALOG BOX */
.ui-tooltip.ui-widget-content {
	border-radius: 6px;
	border: 1px solid #dddddd;
}

.ui-dialog.ui-widget-content {
	min-width: 250px;
}

.ui-dialog .ui-dialog-buttonpane {
	text-align: right;
}

/* TABLE, GRID */
.ui-datatable-data {
	border: none;
}

.ui-datatable-even, .ui-datatable-odd {
	border: none;
}

.ui-datatable-even {
	background-color: #f1f1f1;
}

.ui-datatable-tablewrapper tr th {
	background-color: #41BAEF;
	color: #ffffff;
	font-family: "open_sanssemibold", sans-serif;
	vertical-align: middle;
	border-right: 1px solid #dddddd;
	font-size: 1.2em;
}

.ui-datatable-tablewrapper tr th:last-child {
	border-right: none;
	border-width: none;
}

.ui-datatable tbody td, .ui-datatable tfoot td {
	border: none;
	border-width: none;
}

.ui-selectcheckboxmenu .ui-selectcheckboxmenu-trigger {
	right: 15px;
	top: 5px;
}

.ui-selectcheckboxmenu-panel {
	border: none;
	min-width: 100px;
}

.ui-selectcheckboxmenu, .ui-selectcheckboxmenu .ui-selectcheckboxmenu-label
	{
	color: #fff;
	line-height: 20px;
	font-family: "open_sanslight", sans-serif;
	font-size: 1em;
	font-weight: 600;
}

.ui-column-title {
	line-height: 20px;
	font-family: "open_sanslight", sans-serif;
	font-size: 1em;
	font-weight: 600;
}
/* WIZARD , PANEL */
.ui-wizard-step-titles {
	width: 15%;
	padding: 2px 0px;
	float: left;
	display: block;
	min-height: 300px;
}

.ui-wizard-step-title {
	padding: 0.6em;
	display: block;
	width: 100%;
	background-color: #90CDE7;
	margin-top: 10px;
	border-radius: 0px;
	color: #fff;
	font-family: 'open_sanssemibold', sans-serif;
}

.ui-wizard-step-titles .ui-state-highlight {
	background-color: #41BAEF;
}

.ui-wizard-content {
	float: right;
	margin: 0px;
	width: 85%;
}

.ui-wizard-content .ui-panel {
	min-height: 300px;
	position: relative;
}

.ui-panel .ui-panel-content {
	padding: 2em;
}

.button-section {
	position: absolute;
	bottom: 20px;
}

/* CHART, GRAPH PANEL */
.axis {
	border-radius: 10px;
	padding: 2px 5px;
}

.icon-list {
	text-align: center;
}

.icon-list li {
	display: inline-block;
	cursor: pointer;
	margin: 0 auto;
	padding: 10px;
}

.icon-list li:selected {
	background-color: #dddddd;
}

.icon-list li:hover {
	background-color: #f1f1f1;
}

.icon-list li img {
	width: 32px;
	height: 32px;
}

/*Footer*/
.footer {
	height: 60px;
	padding: 20px 100px 20px;
	font-family: 'open_sanssemibold', sans-serif;
	font-size: medium;
	color: #000;
	background-color: #F8F8F8;
	border-top: 1px solid #f1f1f1;
}

/*Header*/
.header {
	color: #000000;
}

.navbar-brand {
	padding-top: 20px;
}

.navbar-brand img {
	width: 250px;
}

.navbar-nav>li>a {
	line-height: 27px;
}

.useravatar {
	width: 24px;
	height: 24px;
}
.ui-panelgrid-cell{
	border-width: 1px;
	border-style: solid;
	border-color: inherit;
	padding: 4px 10px;
	width: 80px;
}
.ui-paginator .ui-paginator-current, .ui-paginator .ui-paginator-rpp-options {
    margin-left: 0em;
    margin-right: 0em;
    background-image: none;
}